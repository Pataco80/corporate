{
  "name": "corporate",
  "version": "1.0.0",
  "description": "Corporate site for the course Learn PSD to Responsive Parallax HTML, SASS, Gulp, NPM",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "gulp": "gulp",
    "preinstall": "mkdir -p src/scss/partials/font-awesome && mkdir -p src/js && mkdir -p src/img && mkdir -p src/fonts",
    "postinstall": "touch src/index.html && npm run sassfiles && npm run jsfiles && npm run fontAwesome",
    "sassfiles": "touch src/scss/main.scss src/scss/partials/_variables.scss src/scss/partials/_mixins.scss",
    "jsfiles": "touch src/js/main.js",
    "fontAwesome": "cp -r node_modules/font-awesome-sass/assets/stylesheets/ src/scss/partials && mv src/scss/partials/_font-awesome.scss src/scss/font-awesome.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pataco80/corporate.git"
  },
  "keywords": [
    "PSD",
    "Template",
    "to",
    "HTML",
    "SASS",
    "Gulp",
    "NPM"
  ],
  "author": "Ricardo Do Vale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pataco80/corporate/issues"
  },
  "homepage": "https://github.com/Pataco80/corporate#readme",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "font-awesome-sass": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^4.0.0",
    "gulp-newer": "^1.3.0",
    "gulp-sass": "^3.1.0",
    "merge-stream": "^1.0.1",
    "respond.js": "^1.4.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "respond.js": "^1.4.2"
  }
}
